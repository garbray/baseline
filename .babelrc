{
  "presets": [
    [
      "env",
      {
        "targets": {
          "browsers": "last 1 versions"
        },
        "loose": true,
        "modules": false
      }
    ]
  ],
  "env": {
    "test": {
      "plugins": ["transform-es2015-modules-commonjs"]
    },
    "server": {
      "plugins": ["transform-es2015-modules-commonjs"]
    }
  },
  "ignore": ["node_modules"]
}
